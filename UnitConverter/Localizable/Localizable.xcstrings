    "Fuel Efficiency - Mass" : {
      "extractionState" : "manual",
      "localizations" : {
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Yakıt Verimliliği - Kütle"
          }
        }
      }
    },
    "Fuel Efficiency - Volume" : {
      "extractionState" : "manual",
      "localizations" : {
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Yakıt Verimliliği - Hacim"
          }
        }
